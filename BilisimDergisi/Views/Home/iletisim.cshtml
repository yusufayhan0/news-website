@model WebApplication1.DAL.iletisim
@{
    ViewBag.Title = "İletisim";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>İletişim</h2>

<iframe src="@(Model.harita)" class="col-lg-12 col-md-12 col-sm-12" height="400" frameborder="0" style="border:0; margin-top:20px;" allowfullscreen></iframe>

<div class="col-lg-8 col-md-8 col-sm-8" style="margin-top:20px;">
    <div class="footer_widget wow fadeInRightBig">
        
        <p>@Model.yazi</p>
        <address>
            @Model.adres
            <br />Telefon: @Model.tel
        </address>
    </div>
</div>